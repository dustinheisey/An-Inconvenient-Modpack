{
  "name": "inconvenient-modpack",
  "version": "0.6.8",
  "scripts": {
    "format:check": "npx prettier --check '**/*.{js,json,md}'",
    "format:write": "npx prettier --write '**/*.{js,json,md}'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "lint-staged": {
    "**/*.{js,json,md}": "prettier --write"
  }
}
